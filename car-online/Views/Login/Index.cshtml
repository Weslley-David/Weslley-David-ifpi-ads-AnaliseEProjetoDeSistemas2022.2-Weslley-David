@model IEnumerable<car_online.Models.Login>

@{
    ViewData["Title"] = "Index";
}
<div  class="text-center">
<h1>Login</h1>
<div>
    <p><input type="text" class=".bg-transparent border-0 rounded sm" placeholder="email"></p>
    <p><input type="password" class=".bg-transparent border-0 rounded md" placeholder="password"></p>
    <p><button class="btn btn-primary" onclick="location.href = './../';">Signin</button></p>
</div>

<p>
    <a asp-action="Create">Signup</a>
</p>
</div>
